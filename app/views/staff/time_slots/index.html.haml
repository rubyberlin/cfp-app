#proposal
  .row
    .col-md-12
      .page-header.clearfix
        %h1= "#{event} Time Slots"

        .tab-content
          #time-slot.tab-pane.active
            .row
              .col-md-12
                = render partial: 'time_slots'

        #time-slot-edit-dialog.modal.fade{ role: 'dialog', tabindex: '-1', 'aria-labelledby': 'time-slot-edit-dialog-label' }
          .modal-dialog{ role: 'document' }
        #time-slot-new-dialog.modal.fade{ role: 'dialog', tabindex: '-1', 'aria-labelledby': 'time-slot-new-dialog-label' }
          .modal-dialog{ role: 'document' }
